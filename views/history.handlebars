<!doctype html>

<html>
<head>
	<title>History</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="css/bootstrap.css" rel="stylesheet">
<link href="css/bootstrap-theme.css" rel="stylesheet">
<link href="css/hci-friends.css" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->        
</head>

<body>

  <div class="container our-header">
    <div class="jumbotron">
      <h2>History</h2>
    </div>
  </div>

<!-- Nav tabs -->
<div class="container">
<ul class="nav nav-tabs">
  <li class="active"><a href="#1month" data-toggle="tab">Last Month</a></li>
  <li><a href="#3months" data-toggle="tab">3 Months</a></li>
  <li><a href="#6months" data-toggle="tab">6 Months</a></li>
</ul>

<!-- Tab panes -->
<div class="tab-content">
  <div class="tab-pane active" id="1month">
    <p>Medication Missed:<br>
    {{#each Medicine}}
    {{name}}: 3<br>
    {{/each}}
    <br>
    Symptoms Reported:<br>
    {{#each Symptoms}}
    {{name}}: 2<br>
    {{/each}}</p>
  </div>
  <div class="tab-pane" id="3months">
    <p>Medication Missed:<br>
    {{#each Medicine}}
    {{name}}: 9<br>
    {{/each}}
    <br>
    Symptoms Reported:<br>
    {{#each Symptoms}}
    {{name}}: 4<br>
    {{/each}}</p>
  </div>
  <div class="tab-pane" id="6months">
    <p>Medication Missed:<br>
    {{#each Medicine}}
    {{name}}: 18<br>
    {{/each}}
    <br>
    Symptoms Reported:<br>
    {{#each Symptoms}}
    {{name}}: 8<br>
    {{/each}}</p>
  </div>
	<div class="btn-group btn-group-justified">
		<a class="btn btn-primary btn-lg" href="index">Back</a>
	</div>
</div>
	</body>
	<script src="js/jquery-1.11.0.js"></script>
	<script src="js/bootstrap.js"></script>
	<script src="js/hci-friends.js"></script>
</html>
